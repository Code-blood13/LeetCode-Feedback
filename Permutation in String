    bool checkInclusion(string s2, string s1) {
int size1=s1.length();
 int size2=s2.length();
int arr[26]={0};
for(int i=0;i<size2;i++){
int index=s2[i]-'a';
arr[index]++;
} 
for(int j=0;j<=size1-size2;j++){
int arr1[26]={0};

for(int p=0;p<size2;p++){

int index2=s1[j+p]-'a';

arr1[index2]++;
}

for(int k=0;k<26;k++){
 if(arr1[k]!=arr[k]){
    break;
 }
 if(k==25){
    return true;
}
}  
}       
 return false; 
 }
